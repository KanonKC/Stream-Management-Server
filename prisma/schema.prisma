// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ShowImage {
  id        Int      @id @default(autoincrement())
  twitchId  String?
  username  String?
  imageUrl  String
  timestamp DateTime @default(now())
}